        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "2amigos/yii2-tinymce-widget": "*",  
        "2amigos/qrcode-library": "~1.1",
		"bubasuma/yii2-simplechat": "~2.0.0",    
        "kartik-v/yii2-widget-datepicker": "@dev",
        "kartik-v/yii2-mpdf": "*",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "kartik-v/yii2-dialog": "*",
        "kartik-v/yii2-widget-growl": "*",
		"kartik-v/yii2-export": "*",
		"kartik-v/yii2-widget-spinner": "@dev",	
		"kartik-v/yii2-widget-select2": "dev-master",				
        "codemix/yii2-excelexport": "^2.6",
        "rmrevin/yii2-fontawesome": "~2.17",
        "kartik-v/yii2-grid": "@dev",
		"vilochane/yii2-barcode-generator": "dev-master", 
		"spanjeta/yii2-backup": "*",
		"philippfrenzel/yii2fullcalendar":"*",
		"freesoftwarefactory/yii2-select3": "1.0007", 
		"newerton/yii2-fancybox": "dev-master",	      
		"mpdf/mpdf":"*"
		},
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
